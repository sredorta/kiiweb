{
  "index": "/index.html",
  "navigationUrls": [
    "/**",   
    "!/server/**",
    "!/assets/**",
    "!/**/*.*"
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/assets/i18n/*.json",
          "/assets/kiilib/i18n/*.json"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "api-performance",
    "urls": [
    ],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 100,
      "maxAge": "3d"
    }
  },
  {
    "name": "api-freshness",
    "urls": [
      "/server/public/**", 
      "/server/api/initial",
      "/server/api/stats"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 100,
      "maxAge": "3d",
      "timeout": "10s"
    }
  }
  ]

}
